    "eslint-config-vinta": "^1.4.5",
    "expose-loader": "~0.7.3",
    "file-loader": "~4.2.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "jquery": "~3.4.0",
    "node-sass": "^4.13.1",
    "popper.js": "~1.15.0",
    "postcss": "~7.0.13",
