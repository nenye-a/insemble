{
  "compilerOptions": {
    "target": "ES2015",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
